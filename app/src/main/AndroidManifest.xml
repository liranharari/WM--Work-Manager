<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="liran.com.wm_workmanager" >
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_wm_logo"
        android:label="@string/app_name"
        android:logo="@mipmap/ic_launcher_wm_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkActivity" >
        </activity>
        <activity android:name=".ManagerLoginActivity" >
        </activity>
        <activity android:name=".CostumerInfoActivity" >
        </activity>
        <activity android:name=".EditCostumerActivity" >
        </activity>
        <activity android:name=".AddNewCostumerActivity" >
        </activity>
        <activity android:name=".MenuActivity" >
        </activity>
        <activity android:name=".WorkStatusActivity" >
        </activity>
        <activity android:name=".MakeReminderActivity" >
        </activity>
        <activity android:name=".SendReminderActivity" >
        </activity>
        <activity android:name=".SignInActivity" >
        </activity>
        <activity android:name=".NormalLoginActivity" >
        </activity>
    </application>

</manifest>
